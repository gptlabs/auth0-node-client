    "@typescript-eslint/parser": "^5.44.0",
    "debug-logging": "^4.1.2",
    "eslint": "^8.28.0",
    "jwt-decode": "^3.1.2",
    "node-fetch": "^3.3.0",
    "open": "^8.4.0",
    "prompts": "^2.4.2",
